{
  "name": "hyperbeam-discord-bot",
  "version": "1.0.0",
  "description": "A template for slash-create",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run build && pnpm run start",
    "run": "cd dist && node index.js",
    "build": "pnpm exec tsc",
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "lint": "pnpm exec eslint --ext .ts ./src",
    "lint:fix": "pnpm exec eslint --ext .ts ./src --fix"
  },
  "dependencies": {
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "slash-create": "^5.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "slash-up": "^1.2.0",
    "typescript": "^4.7.4"
  }
}